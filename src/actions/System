import {
    PRELOADER_ON,
    PRELOADER_OFF
} from '../constants/System'


export function preloaderOn() {

    return (dispatch) => {

        dispatch({
            type: PRELOADER_ON
        })
    }
}

export function preloaderOff() {

    return (dispatch) => {

        dispatch({
            type: PRELOADER_OFF
        })
    }
}
